<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
  
  <configuration>
  	
  	<!-- xml파일 mapper namespace 별칭지정 -->
  	<settings>
		<setting name="cacheEnabled" value="false" /> 
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>
	<typeAliases>
		<typeAlias type="boardt.model.BTBean" alias="BTBean"/>
		<typeAlias type="member.model.MemberBean" alias="MyMember"/>
	</typeAliases>
	
  	<mappers>
		<mapper resource="mybatis/boards.xml"/>
		<mapper resource="mybatis/boardst.xml"/>
		<mapper resource="mybatis/boardt.xml"/>
		<mapper resource="mybatis/admin.xml"/>
		<mapper resource="mybatis/student.xml"/>
		<mapper resource="mybatis/teacher.xml"/>
		<mapper resource="mybatis/login.xml"/>
		<mapper resource="mybatis/subject.xml"/>
		<mapper resource="mybatis/courses.xml"/>
		<mapper resource="mybatis/course.xml"/>
		<mapper resource="mybatis/user.xml"/>
		<mapper resource="mybatis/member.xml"/>
		<mapper resource="mybatis/board.xml"/>
		<mapper resource="mybatis/accounts.xml"/>
		<mapper resource="mybatis/boarda.xml"/>
		<mapper resource="mybatis/teachers.xml"/>
		<mapper resource="mybatis/order.xml"/>
		<mapper resource="mybatis/orderdetails.xml"/>
	</mappers>
	

	
  </configuration>
  